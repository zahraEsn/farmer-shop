<div class="my-5">
	<h1>Products</h1>
	<div class="row d-flex align-items-center py-4">
		<% if(products.length){ %>
			<% products.forEach((product)=>{ %>
				<div class="col-3">
					<img class="d-block" src="/images/<%= product.image %>" alt="">
					<p class="ps-2" style="color: #679e9b;" href="#"><%= product.title %></p>
				</div>
			<% }) %>
		<% } %>
	</div>
</div>